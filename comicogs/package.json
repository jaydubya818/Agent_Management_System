{
  "name": "comicogs-demos",
  "version": "1.0.0",
  "description": "Comicogs Ultimate Platform - Complete Demo Collection",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8080",
    "dev": "next dev",
    "serve": "python3 -m http.server",
    "build": "npm run optimize:assets && next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production npm run optimize:assets && next build",
    "analyze": "npm run build:analyze && open bundle-analyzer-report.html",
    "bundle:check": "bundlesize",
    "bundle:report": "webpack-bundle-analyzer .next/static/chunks/",
    "optimize:assets": "node scripts/optimize-assets.js optimize",
    "optimize:analyze": "node scripts/optimize-assets.js analyze",
    "optimize:audit": "node scripts/optimize-assets.js audit",
    "optimize:all": "node scripts/optimize-assets.js all",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:accessibility": "playwright test accessibility.spec.ts",
    "test:responsive": "playwright test responsive-design.spec.ts",
    "perf:audit": "lighthouse http://localhost:3000 --output-path=./performance-audit.html",
    "perf:vitals": "node scripts/measure-web-vitals.js"
  },
  "keywords": [
    "comicogs",
    "comics",
    "marketplace",
    "ai",
    "demo"
  ],
  "author": "Comicogs Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
